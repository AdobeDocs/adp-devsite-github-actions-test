          },
          {
            "label": "Adobe Express Embed SDK",
            "icon": "../images/adobe-express.svg"
          }
        ]
      },
      "RequestAccess": {
        "title": "Get credentials",
        "paragraph": "Create unique credentials that you will use to call Adobe Express Embed SDK from your application."
      },
      "RequestAccessRestrictedAccess": {
        "title": "Restricted access",
        "buttonLabel": "You do not have access to this project. Please contact your administrator for more information."
      },
      "RequestAccessRestrictedAccessProducts": {
        "label": "Included products and services",
        "product": [
          {
            "label": "Adobe Express Embed SDK",
            "icon": "../images/adobe-express.svg"
          },
          {
            "label": "Adobe Express Embed SDK",
            "icon": "../images/adobe-express.svg"
          }
        ]
      }
    }
  }
